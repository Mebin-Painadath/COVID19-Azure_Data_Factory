		"StorageAccount_Data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorageAccount_Data'"
		},
		"StorageAccount_EastUS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorageAccount_EastUS'"
